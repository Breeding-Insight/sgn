    $('#upload_phenotype_spreadsheet_file_form').iframePostForm({
	json: true,
	post: function () {
	},
	complete: function (response) {
	    $('#working_modal').modal("hide");
            if (response.error) {
		alert(response.error);
                if (response.error_list_html) {
		     jQuery("#upload_phenotype_spreadsheet_error_display tbody").html(response.error_list_html);
                     jQuery("#upload_phenotype_spreadsheet_error_display").modal("show");
                }
		//return;
            }
            if (response.success) {
		alert("Spreadsheet file stored successfully");
		jQuery('#upload_phenotype_spreadsheet_dialog').modal("hide"); 
                window.location.reload();
            }
	}
    });

