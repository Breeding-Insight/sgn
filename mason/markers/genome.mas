<%args>
  $xrefs
</%args>
<&| /page/info_section.mas, title => "Genomic locations" &>
% for my $xref ( @browser_xrefs ) {
    <& /ambikon/xrefs/consume/xref.mas, xref => $_ &>
% }
</&>
<%init>
  use Data::Visitor::Callback;

  my @browser_xrefs;
  Data::Visitor::Callback->new(
      object_no_class => 'visit_ref',
      'Ambikon::Xref' => sub {
          push @browser_xrefs, $_ if $_->has_tag('genome browser');
      },
  )->visit( $xrefs );

</%init>
