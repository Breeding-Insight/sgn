<%args>
  $xrefs
</%args>

<& /ambikon/xrefs/gbrowse2/xref_set/rich.mas, xrefs => \@gbrowse_gene_model_xrefs &>

<%init>

  # look up any gbrowse cross-refs for this locus id, if any
  my @gbrowse_gene_model_xrefs =
     grep {
           $_->subsite->name eq 'gbrowse2'
        && $_->data_source->description =~ /genomic/i
     } @$xrefs;
</%init>
